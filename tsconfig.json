{
    "compilerOptions": {
      "target": "es2015",
      "module": "commonjs",
      "moduleResolution": "node",
      "jsx": "react",
      "experimentalDecorators": true,
      "allowSyntheticDefaultImports": true,
      "noUnusedLocals": true,
      "strictNullChecks": false,
      "noImplicitReturns": false,
      "noImplicitAny": false
    },
    "include": ["src", "typings", "package.json", "fuse.ts"],
    "exclude": [
      "node_modules"
    ]
  }
